# Example environment file for frontend PRODUCTION
# Copy this file to .env.production for production deployment

# API Configuration - this will be automatically set during deployment
# but can be overridden here if needed
REACT_APP_API_URL=https://servitec-map-api-ypzdt6srya-uc.a.run.app/api/v1

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_AUTH=true
REACT_APP_DEMO_MODE=false

# Map Configuration
REACT_APP_DEFAULT_MAP_CENTER=20.6596,45.3476
REACT_APP_DEFAULT_ZOOM=12
REACT_APP_MAX_ZOOM=19
REACT_APP_MIN_ZOOM=8

# UI Configuration
REACT_APP_THEME=light

# Production Build Settings
GENERATE_SOURCEMAP=false
REACT_APP_ENV=production 